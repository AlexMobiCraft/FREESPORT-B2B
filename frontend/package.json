{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix --max-warnings=0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "test": "cross-env VITEST=true vitest run",
    "test:watch": "cross-env VITEST=true vitest",
    "test:coverage": "cross-env VITEST=true vitest run --coverage",
    "test:ui": "cross-env VITEST=true vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "generate:types": "openapi-typescript ../docs/api-spec.yaml -o ./src/types/api.generated.ts",
    "postinstall": "node scripts/install-lightningcss.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@types/axios": "^0.9.36",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "embla-carousel": "^8.6.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "jspdf": "^4.0.0",
    "lucide-react": "^0.553.0",
    "motion": "^12.23.25",
    "next": "^15.5.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "axe-core": "^4.11.0",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "msw": "^2.12.2",
    "openapi-typescript": "^7.10.1",
    "prettier": "^3.3.3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.15",
    "vitest-axe": "^0.1.0"
  },
  "optionalDependencies": {
    "lightningcss-linux-x64-gnu": "1.30.1",
    "lightningcss-linux-x64-musl": "1.30.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0 --fix --no-warn-ignored",
      "prettier --write"
    ],
    "*.{json,md,css,scss,html,yml,yaml}": [
      "prettier --write --ignore-unknown"
    ]
  }
}
